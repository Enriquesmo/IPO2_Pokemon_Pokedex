                <RowDefinition Height="80*"/>
            </Grid.RowDefinitions>
            <Image x:Name="imFondo" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center"
                Grid.RowSpan="4" Grid.ColumnSpan="3" Source="/Assets/back.jpg" Stretch="None" d:IsLocked="True"/>
            <ProgressBar x:Name="pbHealth" Grid.Column="1" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" Background="#33E81717" Foreground="#FFFB0707" Value="60"/>
            <ProgressBar x:Name="pbEnergy" Grid.Column="1" Margin="0,0,0,0" Grid.Row="1" VerticalAlignment="Stretch" Background="#33FFF500" Foreground="#FFFFEA04" Value="100"/>
            <Image x:Name="imCorazon" Grid.Column="0" Grid.Row="0" VerticalAlignment="Stretch" Source="/Assets/corazon.png" d:IsLocked="True"/>
            <Image x:Name="imRPotion" Grid.Column="2" Grid.Row="0" VerticalAlignment="Stretch" Source="/Assets/pocion_roja.png" PointerReleased="usePotionRed"/>
            <Image x:Name="imRayo" Grid.Column="0" Grid.Row="1" VerticalAlignment="Stretch" Source="/Assets/rayo (2).png" FocusVisualPrimaryBrush="White"/>
            <Image x:Name="imYPotion" Grid.Column="2" Grid.Row="1" VerticalAlignment="Stretch" Source="/Assets/pocion_amarilla.png" PointerReleased="UseEnergyPotion"/>
            <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.Column="1">
                <Button x:Name="btProteccion" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" FontSize="45" Content="Protección" FontWeight="Bold" FontFamily="Bernard MT Condensed" Foreground="White" BorderBrush="{x:Null}" Click="btProteccion_Click" RequestedTheme="Dark" Margin="0,0,0,0"/>
                <Button x:Name="btDescanso" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" FontSize="45" Content="Descanso" FontWeight="Bold" FontFamily="Bernard MT Condensed" Background="#FF665694" BorderBrush="#00000000" Click="btDescanso_Click" RequestedTheme="Dark"/>
